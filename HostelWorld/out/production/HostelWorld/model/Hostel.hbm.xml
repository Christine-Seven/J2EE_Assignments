<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="model">
    <class name="Hostel" table="hostel">
        <id name="id" column="id"></id>
        <property name="hostelNum" column="hostelNum"></property>
        <property name="hostelPassword" column="hostelPassword"></property>
        <property name="totalRoomNum" column="totalRoomNum"></property>
        <property name="spareRoomNum" column="spareRoomNum"></property>
        <property name="profit" column="profit"></property>
        <property name="province" column="province"></property>
        <property name="city" column="city"></property>
        <property name="address" column="address"></property>
    </class>

</hibernate-mapping>